package Y2021M5D16_Reflect;
/**
 * 	JDK5之后推出了一个新的特性:
 * 	可变参数
 * @author Grant·Vranes
 *
 */
public class ArgDemo {
	public static void main(String[] args) {
		dosome("a");
		dosome("a","b");
		dosome("a","b","c");
	}
	
	public static void dosome(String... s) {
		System.out.println(s.length);
	}
}

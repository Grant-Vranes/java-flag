package Y2021M2D28;
//学生类
public class Student extends Person {
	//成员变量
	String stuId;
	//构造方法
	public Student(String name,int age,String address) {
		super(name,age,address);
		this.stuId = stuId;
	}
	//方法
	public void sayHi() {
		System.out.println("姓名:"+name+" 年龄:"+age+" 地址:"+address);
	}
}

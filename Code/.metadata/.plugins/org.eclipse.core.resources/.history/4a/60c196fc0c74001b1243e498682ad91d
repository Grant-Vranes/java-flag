package Y2021M2D21;
//方法演示
public class Demo_1 {
	public static void main(String[] args) {
		say();
		say("Grant");
	}
	
	//无参无返回值
	public static void say() {
		System.out.println("Java_Flag");
	}
	
	//有参无返回值
	public static void say(String name) {
		System.out.println(name + "is the King");
	}
}

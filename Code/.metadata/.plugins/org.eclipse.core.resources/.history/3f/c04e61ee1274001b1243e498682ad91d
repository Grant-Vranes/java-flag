package Y2021M2D20;

public class Demo_13 {
	public static void main(String[] args) {
		//找到数组元素的最小值，并放在第一位
		int[] arr = new int[10];
		for(int i = 0; i < arr.length; i++) {
			arr[i] = (int)(Math.random()*100);
			System.out.println(arr[i]);
		}
		
		int min = arr[0];
		for(int i = 1; i < arr.length; i++) {
			if(arr[i] < min) {
				min = arr[i];
			}
		}
		System.out.println("min = " + min);
	}
}

package Y2021M3D8;
//多态的演示
public class MultiTypeDemo {
	public static void main(String[] args) {
		Aoo o = new Boo();//向上造型
		Boo o1 = (Boo)o;//o指向的对象就是Boo类型
		Inter1 o2 = (Inter1)o;//o指向的对象实现了Inter1接口
		Coo o3 = (Coo)o;//指向对象既不是该类型也没有实现该接口，ClassCastException类型转换
		//System.out.println(o instanceof Coo);
	}
}

interface Inter1{
	
}
class Aoo{
	
}
class Boo extends  Aoo implements Inter1{
	
}
class Coo extends Aoo{
}
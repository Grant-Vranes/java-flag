	//构造方法
	public Sky(){//属性都写死了，不需要参数
		/*
		width = 400;//天空的宽高固定，可以写死
		height = 700;
		x = 0;
		y = 0;
		*/
		super(400,700,0,0);
		speed = 1;
		y1 = -700;
	}
	//移动
	public void step() {
		
	}
}

package Shoot;
//蜜蜂
public class Bee extends FlyingObject{
	private int xSpeed;//x坐标移动速度
	private int ySpeed;//y坐标移动速度
	private int awardType;//奖励类型（0或1）
	public Bee(){
		super(60,50);
		xSpeed = 1;
		ySpeed = 2;
		awardType = (int)(Math.random()*2); //0或1
	}
	public void step() {
		
	}
}

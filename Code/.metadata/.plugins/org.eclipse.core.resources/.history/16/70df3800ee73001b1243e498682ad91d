package Y2021M2D20;

public class Demo_11 {
	public static void main(String[] args) {
		//数组的复制
		int[] a = {10,20,30,40,50};
		int[] a1 = new int[6];
		System.arraycopy(a,1,a1,0,4);
		/*
		 * a:源数组
		 * 1:源数组的起始下标
		 * a1:目标数组
		 * 0:目标数组的起始下标
		 * 4:要复制的元素个数
		 */
		for(int v : a1) {
			System.out.print(v+"\t");
		}
	}
}

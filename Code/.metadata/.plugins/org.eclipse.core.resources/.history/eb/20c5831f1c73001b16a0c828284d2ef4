package Y2021M2D20;

public class Demo_1 {
	public static void main(String[] args) {
		/*
		 * 1.算数运算符：+，-，*，/，%，++，--
		 * 		1）%：取模/取余运算，余数为0即为整除
		 * 		2）++/--：自增1/自减1，可在变量前也可在后
		 * 			a）单独使用时，在前在后 都一样
		 * 			b）被使用时，在前在后不一样
		 * 				a++的值为a
		 * 				++a的值为a+1			
		 * 	
		 */
		int a = 5, b = 5;
		a++;
		++b;
		System.out.println(a);
		System.out.println(b);
		
	}
}
